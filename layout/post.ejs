<main>
    <% if (is_post()) { %>
    <h1>
        <%= page.title %>
    </h1>
    <% } %>
    <p>
        <i><time><%= date(page.date, 'YYYY/MM/DD' ) %></time></i>
    </p>
    <%- page.content %>
    <% if (theme.twikoo.enable || false) { %>
        <%- partial('components/twikoo') %>
    <% } %>
</main>